<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@../style/sidePanel.css" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="9999" prefWidth="60.0" spacing="30.0" styleClass="Side-Panel-Layout" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.compiler.gui.controller.SidePanelController">
   <children>
      <Button styleClass="no-display-Button" alignment="CENTER" mnemonicParsing="false"  fx:id="newFile"/>
      <Button styleClass="no-display-Button" alignment="CENTER" mnemonicParsing="false"  fx:id="open" onAction="#loadFile" />
      <Button styleClass="no-display-Button" alignment="CENTER" mnemonicParsing="false"  fx:id="save" onAction="#saveFile"/>
      <Button styleClass="no-display-Button" alignment="CENTER" mnemonicParsing="false"  fx:id="compile" />
   </children>
   <padding>
      <Insets top="60.0" />
   </padding>
</VBox>
